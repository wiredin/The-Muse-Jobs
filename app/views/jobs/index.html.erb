<h1>The Muse Jobs</h1>



<ul class="list-unstyled">
<% @jobs.each do |job| %>
  <li class="job-list">
    <span class="company"><%= job["company"]["name"] %></span>
    <span class="job-name"><%= job["name"] %></span>
  </li>
<% end %>
</ul>


<nav aria-label="Page navigation">
  <ul class="pagination">
  <% display_pages.each do |p| %>
    <li <%= 'class=active' if p == @page  %>>
      <%= link_to p, jobs_path(page: p) %> 
    </li>
  <% end %>
  </ul>
</nav>
